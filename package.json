{
  "name": "air-quality",
  "version": "1.0.0",
  "description": "Air Quality App",
  "author": {
    "name": "Uzair"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:docker": "docker-compose up --build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "jest --config ./jest-e2e.json",
    "migration": "npm run exec:migrations && npm run exec:seeder",
    "migration:run": "node_modules/.bin/sequelize db:migrate --migrations-path=./dist/database/migrations",
    "migration:drop": "node_modules/.bin/sequelize db:migrate:undo",
    "migration:create": "node_modules/.bin/sequelize migration:generate --name",
    "migration:seed:create": "node_modules/.bin/sequelize seed:generate --name",
    "migration:seed-dev": "node_modules/.bin/sequelize db:seed:all --seeders-path ./dist/database/seeders",
    "prepare:husky": "husky install",
    "exec:migration": "docker exec -it app npm run migration:run",
    "exec:seeder": "docker exec -it app npm run migration:seed-dev",
    "api:exec": "docker exec -it app sh",
    "detect-secrets": "detect-secrets-launcher src/*/**",
    "test:e2e": "docker exec -it app npm run e2e"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "9.2.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "6.1.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "detect-secrets": "^1.0.6",
    "dotenv": "^10.0.0",
    "helmet": "^4.6.0",
    "nestjs-pino": "^3.1.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pino-http": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.27.0",
    "sequelize-cli": "^6.5.2",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.3",
    "@types/eslint": "^8.4.10",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.0.0",
    "@types/pg": "^8.6.1",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/$1"
    }
  }
}
